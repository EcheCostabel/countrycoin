{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint src/**/*.jsx --no-ignore --report-unused-disable-directives --max-warnings 100",
    "preview": "vite preview"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "6.4.0",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@hookform/resolvers": "3.1.1",
    "@tanstack/react-query": "4.29.19",
    "axios": "1.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.45.1",
    "react-icons": "4.10.1",
    "react-router-dom": "6.14.1",
    "swiper": "10.0.4",
    "yup": "1.2.0"
  },
  "devDependencies": {
    "@iconify/react": "4.1.1",
    "@types/react": "18.0.37",
    "@types/react-dom": "18.0.11",
    "@vitejs/plugin-react": "4.0.0",
    "autoprefixer": "10.4.14",
    "daisyui": "3.1.7",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.3.4",
    "postcss": "8.4.24",
    "tailwindcss": "3.3.2",
    "vite": "4.3.9"
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react/jsx-runtime",
      "plugin:react-hooks/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module",
      "settings": {
        "react": {
          "version": "18.2"
        }
      },
      "plugins": [
        "react-refresh"
      ],
      "rules": {
        "react-refresh/only-export-components": "warn"
      }
    }
  }
}
